'use strict';

var app = require('../app');
var socketIo = require('socket.io') // io 利用事件机制来通信
var http = require('http');
var sqlConfig = process.env.mysql;
var nodeConfig = process.env.node;

let server, io

server = http.Server(app.callback());
io = socketIo(server);
server.listen(8099, function () {
	console.log("Server at http://127.0.0.1:"+8099+", press [CTRL+C] to stop server");
});
